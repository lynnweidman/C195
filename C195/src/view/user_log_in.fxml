<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="50.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserController">
   <children>
      <AnchorPane fx:id="LogInPage" layoutY="8.0" prefHeight="393.0" prefWidth="600.0">
         <children>
            <Label fx:id="logInLabel" contentDisplay="CENTER" layoutX="7.0" layoutY="6.0" prefHeight="35.0" prefWidth="586.0" text="Log In">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Label fx:id="userNamLabel" layoutX="52.0" layoutY="66.0" text="Username">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="passwordLabel" layoutX="57.0" layoutY="106.0" text="Password">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="userNameTxt" layoutX="200.0" layoutY="67.0" />
            <TextField fx:id="passwordTxt" layoutX="200.0" layoutY="107.0" />
            <Button fx:id="enterButton" layoutX="244.0" layoutY="143.0" mnemonicParsing="false" onAction="#enterButtonPressed" text="Enter" />
            <Label fx:id="zoneIdLabel" layoutX="200.0" layoutY="15.0" text="Zone_ID" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
